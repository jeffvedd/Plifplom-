    "version": ">= 3.0.0"
  },
  "build": {
    "production": {
      "android": {
        "image": "ubuntu-22.04-android-32-ndk-r25c",
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      },
      "env": {
        "EXPO_NO_DEV_SERVER": "true"
      }
